{
  "name": "react-demo",
  "version": "1.0.0",
  "description": "React Scripts 版本的 Meta Map 组件演示项目，支持联调热更新。",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-scripts": "5.0.1",
    "tlbs-map-react": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set GENERATE_SOURCEMAP=false && react-scripts start",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "build:library": "tsc --project ../tsconfig.json --outDir ../dist && xcopy \"..\\src\\components\\*.css\" \"..\\dist\\components\\\" /Y",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm run start",
    "dev:link": "node scripts/dev-link.js",
    "dev:windows": "start-dev.bat",
    "setup": "npm install && npm run link:library",
    "link:library": "npm link ../",
    "unlink:library": "npm unlink @ai-map/meta_map",
    "test:setup": "node test-setup.js",
    "test:data": "node test-data-loading.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "keywords": [
    "react",
    "react-scripts",
    "map",
    "clustering",
    "visualization",
    "demo",
    "hot-reload"
  ],
  "author": "",
  "license": "MIT"
}
